    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3"
  },
  "scripts": {
    "build": "webpack --mode production --config ./config/webpack.config.prod.js",
    "develop": "webpack-dev-server --open --mode development --config ./config/webpack.config.dev.js",
